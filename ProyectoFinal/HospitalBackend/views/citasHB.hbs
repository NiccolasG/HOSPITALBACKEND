<h1>HOSPITAL BACKEND</h1>
<h2>LISTADO DE CITAS HOSPITAL</h2>
<table>
<thead>
<tr>
<th>Nombre paciente</th>
<th>Cedula </th>
<th>Nombre medico</th>
<th>Consultorio</th>
<th>Fecha</th>
<th>Opciones</th>
</tr>
</thead>
<tbody>
{{#each citas}}
<tr>
<td>{{nombre}}</td>
<td>{{id_paciente}}</td>
<td>{{nombres}} {{apellidos}}</td>
<td>{{consultorio}}</td>
<td>{{formatoFecha fecha}}</td>
<td><a href="/citas/eliminar/{{id}}">Eliminar</a></td>
</tr>
{{/each}}
</tbody>
</table>
<a class="volver" href="/">Volver</a>
<a href="citas/agregar-cita">Nueva cita</a>